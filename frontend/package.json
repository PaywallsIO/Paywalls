{
  "name": "vite-kea-react-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:kea\" \"npm run dev:vite\" -n KEA,VITE -c green,magenta",
    "dev:kea": "kea-typegen watch --write-paths",
    "dev:vite": "vite",
    "build": "npm run typegen:write && tsc && vite build",
    "start": "serve -s dist",
    "prettier": "prettier --write src",
    "preview": "vite preview",
    "typegen:write": "kea-typegen write --delete --show-ts-errors"
  },
  "dependencies": {
    "@grapesjs/react": "^1.0.0",
    "@mantine/carousel": "^7.11.1",
    "@mantine/charts": "^7.11.1",
    "@mantine/code-highlight": "^7.11.1",
    "@mantine/core": "^7.11.1",
    "@mantine/dates": "^7.11.1",
    "@mantine/dropzone": "^7.11.1",
    "@mantine/form": "^7.11.1",
    "@mantine/hooks": "^7.11.1",
    "@mantine/modals": "^7.11.1",
    "@mantine/notifications": "^7.11.1",
    "@mantine/nprogress": "^7.11.1",
    "@mantine/spotlight": "^7.11.1",
    "@mantine/tiptap": "^7.11.1",
    "@tabler/icons-react": "^3.9.0",
    "@tiptap/extension-link": "^2.4.0",
    "@tiptap/react": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@types/js-cookie": "^3.0.6",
    "dayjs": "^1.11.11",
    "embla-carousel-react": "^8.1.6",
    "fast-deep-equal": "^3.1.3",
    "grapesjs": "^0.21.12",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "kea": "^3.0.2",
    "kea-loaders": "^3.0.1",
    "kea-router": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.4.0",
    "kea-forms": "^3.2.0",
    "kea-test-utils": "^0.2.2",
    "kea-typegen": "^3.1.3",
    "postcss": "^8.4.39",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}